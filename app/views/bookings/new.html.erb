<h1>Edit property</h1>
<div class="form">
  <%= simple_form_for [@flat, @booking], :url => flat_bookings_path,  html: {multipart: true} do |f| %>
    <%= f.input :check_in %>
    <%= f.input :check_out%>
    <%= f.input :user_id %>
    <%= f.input :flat_id%>
    <%= f.input :accepted%>
    <%= f.button :submit %>
  <% end %>
</div>

<%= link_to 'Cancel', new_flat_booking_path %>
